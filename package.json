    "electron": "^6.0.3",
    "electron-builder": "^21.2.0",
    "react-scripts": "^3.3.1",
    "url": "^0.11.0"
  },
  "build": {
    "productName": "example",
    "appId": "com.example.server",
    "files": [
      "build/**/*",
      "package.json"
    ],
