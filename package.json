{
  "name": "react-electron",
  "version": "0.1.0",
  "main": "./public/electron.js",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "antd": "^3.26.7",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-use": "^13.22.4",
    "redux": "^4.0.5",
    "styled-components": "^5.0.0"
